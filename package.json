{
  "name": "mod-icon-library",
  "version": "2.1.18",
  "description": "libreria de iconos personalizados",
  "author": "JModestoAlvarezGomez",
  "license": "MIT",
  "repository": "https://github.com/jmodestoag/mod-icon-library.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "keywords": [
    "iconos, svg"
  ],
  "homepage": "https://github.com/jmodestoag/test-app-react.git",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false  --no-compress --format modern,cjs && npm copy",
    "start": "microbundle-crl watch --css-modules false  --no-compress --format modern,cjs & npm copy",
    "copy": "mkdir -p dist/icons/ && cp -a src/icons/fonts/. dist/fonts/"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^18.0.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^2.1.3",
    "webpack-cli": "^5.0.1"
  },
  "files": [
    "dist"
  ]
}
